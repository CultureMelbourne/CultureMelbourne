                        <div class="col-6" style="background-size: cover; background-repeat: no-repeat;">
                            <img class="img-resize" src="@Url.Content("~/Content/Customer/images/study-case-single/main/vit/1.jpeg")" class="img-fluid rounded-10 wow zoomIn animated" alt="" style="visibility: visible; animation-name: zoomIn;">

