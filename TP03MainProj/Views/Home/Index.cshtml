@{
    ViewBag.Title = "Home Page";
}

<main>
    <!-- content begin -->
    <div class="no-bottom no-top" id="content">

        <div id="top"></div>

        <section class="text-light no-top no-bottom position-relative z-1000">
            <div class="v-center">
                <div class="swiper">
                    <!-- Additional required wrapper -->
                    <div class="swiper-wrapper">
                        <!-- Slides -->
                        <div class="swiper-slide" data-jarallax-element="150">
                            <div class="swiper-inner">
                                <img src="@Url.Content("~/Content/Customer/images/slider/4.jpg")" alt="Slide Image" style="width: 100%; height: auto;">
                                <div class="sw-caption">
                                    <div class="container">
                                        <div class="row gx-5 align-items-center">
                                            <div class="col-lg-8 mb-sm-20">
                                                <div class="subtitle s2 mb-4">Embracing Diversity</div>
                                                <h1 class="slider-title">Explore <span class="underline">East Asian</span> Culture</h1>

                                            </div>
                                            <div class="col-lg-6">
                                                <p class="slider-teaser">Dive into the rich history, arts, and traditions of East Asia and experience its unique charm.</p>
                                                <hr class="s2">
                                                <div class="spacer-10"></div>
                                                <a class="btn-main mb10" href="services.html">Explore Culture</a>&nbsp;
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="sw-overlay s2"></div>
                            </div>
                        </div>

                        <!-- Slides -->
                        <div class="swiper-slide" data-jarallax-element="150">
                            <div class="swiper-inner">
                                <img src="@Url.Content("~/Content/Customer/images/slider/2.jpg")" alt="Slide Image" style="width: 100%; height: auto;">
                                <div class="sw-caption">
                                    <div class="container">
                                        <div class="row gx-5 align-items-center">
                                            <div class="col-lg-8 mb-sm-20">
                                                <div class="subtitle s2 mb-4">Celebrating Traditions</div>
                                                <h1 class="slider-title">Our Stories</h1>
                                            </div>
                                            <div class="col-lg-6">
                                                <p class="slider-teaser">Hear the stories of East Asian immigrants who have made their mark in Melbourne, adding color and diversity to our city.</p>
                                                <hr class="s2">
                                                <div class="spacer-10"></div>
                                                <a class="btn-main mb10" href="services.html">Join Festivities</a>&nbsp;
                                                @*<a class="btn-line mb10" href="contact.html">Free Consultation</a>*@
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="sw-overlay s2"></div>
                            </div>
                        </div>
                        <!-- Slides -->
                        <div class="swiper-slide" data-jarallax-element="150">
                            <div class="swiper-inner">
                                <img src="@Url.Content("~/Content/Customer/images/slider/3.jpg")" alt="Slide Image" style="width: 100%; height: auto;">
                                <div class="sw-caption">
                                    <div class="container">
                                        <div class="row gx-5 align-items-center">
                                            <div class="col-lg-8 mb-sm-20">
                                                <div class="subtitle s2 mb-4">Preserving Heritage</div>
                                                <h1 class="slider-title">Building an Inclusive Community</h1>
                                            </div>
                                            <div class="col-lg-6">
                                                <p class="slider-teaser">Learn how to recognize and oppose discrimination, striving together to create a more inclusive and understanding environment for East Asian immigrants.</p>
                                                <hr class="s2">
                                                <div class="spacer-10"></div>
                                                <a class="btn-main mb10" href="services.html">Support Culture</a>&nbsp;
                                                @*<a class="btn-line mb10" href="contact.html">Free Consultation</a>*@
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="sw-overlay s2"></div>
                            </div>
                        </div>


                    </div>
                    <!-- If we need pagination -->
                    <div class="swiper-pagination"></div>

                    <!-- If we need navigation buttons -->
                    <div class="swiper-button-prev"></div>
                    <div class="swiper-button-next"></div>

                    <!-- If we need scrollbar -->
                    <div class="swiper-scrollbar"></div>
                </div>
            </div>
